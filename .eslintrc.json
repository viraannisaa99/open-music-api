{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "comma-dangle": "off",
    "linebreak-style": 0,
    "max-len": "off",
    "object-curly-newline": "off",
    "no-unused-vars": "off",
    "newline-per-chained-call": "off",
    "no-trailing-spaces": "off"
  }
}
